{
  "name": "@gohorse/npm-i18n",
  "version": "1.4.2",
  "description": "GoHorse is a Git-based blockchain. Library with functionality for internationalization.",
  "bin": "./js/index.js",
  "main": "./js/index.js",
  "types": "./js/index.d.ts",
  "scripts": {
    "format": "prettier --write \"./ts/**/*.ts\" \"./test/**/*.ts\" \"./js/**/*.js\"",
    "lint": "eslint \"./ts/**/*.ts\"",
    "format+lint": "npm run format && npm run lint",
    "test": "jest --config ./jest.config.ts",
    "build": "tsc && copyfiles --flat ./**/translat*.json ./js",
    "build:prod": "npm run build && npm run format+lint && npm run test",
    "start": "ts-node ./ts/index.ts",
    "prepare": "npm run build",
    "version": "npm run build:prod && git add --all",
    "postversion": "git push -u origin HEAD && git push origin --tags"
  },
  "files": [
    "./js/**/*",
    "./translat*.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiocabral/Blockchain.GoHorse.git"
  },
  "author": {
    "name": "Sergio Cabral",
    "email": "gohorse@sergiocabral.com",
    "url": "https://sergiocabral.com/"
  },
  "keywords": [
    "gohorse",
    "coin",
    "blockcahin",
    "cryptocurrency",
    "cabroncoin",
    "sergiocabral"
  ],
  "license": "GoHorse Blockchain Â© 2021 by sergiocabral.com is licensed under CC BY 4.0. To view a copy of this license, visit http://creativecommons.org/licenses/by/4.0/",
  "bugs": {
    "url": "https://github.com/sergiocabral/Blockchain.GoHorse/issues"
  },
  "homepage": "https://github.com/sergiocabral/Blockchain.GoHorse/tree/npm-i18n",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@gohorse/npm-core": "^2.2.0",
    "@sergiocabral/helper": "^9.0.0",
    "tslib": "^2.4.0"
  }
}
