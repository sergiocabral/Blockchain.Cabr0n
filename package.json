{
  "name": "@gohorse/npm-application",
  "version": "4.0.0",
  "description": "GoHorse is a Git-based blockchain. Library with features for building executable applications.",
  "bin": "js/index.js",
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "scripts": {
    "format": "prettier --write \"ts/**/*.ts\" \"test/**/*.ts\" \"js/**/*.js\" \"translat*.json\"",
    "lint": "eslint \"ts/**/*.ts\"",
    "format+lint": "npm run format && npm run lint",
    "test": "jest --config jest.config.ts",
    "build": "tsc",
    "build:prod": "npm run build && npm run format+lint && npm run test",
    "start": "ts-node ts/index.ts /id=*",
    "stop": "ts-node ts/index.ts /id=* /stop",
    "prepare": "npm run build",
    "version": "npm run build:prod && git add --all",
    "postversion": "git push -u origin HEAD && git push origin --tags"
  },
  "files": [
    "js/**/*",
    "translat*.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiocabral/Blockchain.GoHorse.git"
  },
  "author": {
    "name": "Sergio Cabral",
    "email": "gohorse@sergiocabral.com",
    "url": "https://sergiocabral.com/"
  },
  "keywords": [
    "gohorse",
    "coin",
    "blockcahin",
    "cryptocurrency",
    "cabroncoin",
    "sergiocabral"
  ],
  "license": "GoHorse Blockchain Â© 2021 by sergiocabral.com is licensed under CC BY 4.0. To view a copy of this license, visit http://creativecommons.org/licenses/by/4.0/",
  "bugs": {
    "url": "https://github.com/sergiocabral/Blockchain.GoHorse/issues"
  },
  "homepage": "https://github.com/sergiocabral/Blockchain.GoHorse/tree/npm-application",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@gohorse/npm-core": "^5.2.1",
    "@gohorse/npm-database": "^1.0.5",
    "@gohorse/npm-database-elaticsearch": "^1.0.5",
    "@gohorse/npm-i18n": "^2.0.38",
    "@gohorse/npm-log": "^3.1.8",
    "@gohorse/npm-log-console": "^1.1.11",
    "@gohorse/npm-log-database": "^2.0.6",
    "@gohorse/npm-log-file": "^1.2.8",
    "tslib": "^2.4.0",
    "@sergiocabral/helper": "^11.6.2"
  }
}
