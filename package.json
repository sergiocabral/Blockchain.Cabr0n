{
  "name": "@gohorse/npm-core",
  "version": "5.2.4",
  "description": "GoHorse is a Git-based blockchain. Base library with general functionality for the other parts of the system.",
  "bin": "js/index.js",
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "scripts": {
    "format": "prettier --write \"ts/**/*.ts\" \"test/**/*.ts\" \"js/**/*.js\" \"translat*.json\"",
    "lint": "eslint \"ts/**/*.ts\"",
    "format+lint": "npm run format && npm run lint",
    "test": "jest --config jest.config.ts",
    "build": "tsc",
    "build:prod": "npm run build && npm run format+lint && npm run test",
    "start": "ts-node ts/index.ts",
    "prepare": "npm run build",
    "version": "npm run build:prod && git add --all",
    "postversion": "git push -u origin HEAD && git push origin --tags"
  },
  "files": [
    "js/**/*",
    "translat*.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiocabral/Blockchain.GoHorse.git"
  },
  "author": {
    "name": "Sergio Cabral",
    "email": "gohorse@sergiocabral.com",
    "url": "https://sergiocabral.com/"
  },
  "keywords": [
    "gohorse",
    "coin",
    "blockcahin",
    "cryptocurrency",
    "cabroncoin",
    "sergiocabral"
  ],
  "license": "GoHorse Blockchain Â© 2021 by sergiocabral.com is licensed under CC BY 4.0. To view a copy of this license, visit http://creativecommons.org/licenses/by/4.0/",
  "bugs": {
    "url": "https://github.com/sergiocabral/Blockchain.GoHorse/issues"
  },
  "homepage": "https://github.com/sergiocabral/Blockchain.GoHorse/tree/npm-core",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "tslib": "^2.4.1",
    "@sergiocabral/helper": "^11.9.3"
  }
}
